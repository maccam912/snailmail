{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "build/index.js",
  "engines": {
    "node": ">= 20.9.0"
  },
  "scripts": {
    "start": "tsx watch src/index.ts",
    "loadtest": "tsx loadtest/example.ts --room my_room --numClients 2",
    "build": "npm run clean && tsc",
    "clean": "rimraf build",
    "test": "mocha test/**/*.ts --require tsx/cjs",
    "test:helm": "mocha test/helm_test.ts --require tsx/cjs",
    "validate:helm": "./scripts/validate-helm.sh"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.16.0",
    "@colyseus/testing": "^0.16.0",
    "@types/express": "^4.17.1",
    "@types/mocha": "^10.0.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.16.0",
    "@colyseus/playground": "^0.16.0",
    "@colyseus/tools": "^0.16.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.59.0",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.201.1",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.201.1",
    "colyseus": "^0.16.0",
    "express": "^4.18.2"
  }
}
